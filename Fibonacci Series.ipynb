    "n1=int(input(\"Enter the first number of the series: \"))\n",
    "n2=int(input(\"Enter the second number of the series: \"))\n",
    "n=int(input(\"Enter limit: \"))\n",
    "print(n1,n2,end=\" \")\n",
    "n=n-2\n",
    "while(n>0):\n",
    "    t=n1+n2\n",
    "    n1=n2\n",
    "    n2=t\n",
    "    print(t,end=\" \")\n",
    "    n=n-1"
